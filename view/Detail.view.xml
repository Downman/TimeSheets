<mvc:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" controllerName="timesheet_ns.view.Detail">
    <Page id="detailPage" navButtonPress="onNavBack" title="{i18n&gt;detailTitle}" showNavButton="{device&gt;/isPhone}">
        <content>
            <ObjectHeader id="detailHeader" title="{ProjectID}" number="" numberUnit="" introActive="false" titleActive="false" iconActive="false">
                <attributes id="detailAttributes">                
                    <ObjectAttribute id="attribute" text="{i18n&gt;detailText}" active="false">
                    </ObjectAttribute>
                </attributes>
                <firstStatus id="detailStatus">
                    <ObjectStatus id="status" text="">
                    </ObjectStatus>
                </firstStatus>
            </ObjectHeader>
            <IconTabBar id="idIconTabBar" expanded="{device&gt;/isNoPhone}">
    			<items id="detailsItems">
                    <IconTabFilter id="iconTabFilter1" key="selfInfo" icon="sap-icon://hint">
                        <content>
							<f:SimpleForm id="iconTabFilter1form" minWidth="1024" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1">
								<f:content>
									<List noDataText="Drop list items here" id="__list3">
									    <customData>
									        <core:CustomData key="sap-ui-fastnavgroup" value="true" writeToDom="true" id="__data5"/>
									    </customData>
									    <items/></List>
									<core:ExtensionPoint name="extIconTabFilterForm1"/>
								</f:content>
							</f:SimpleForm>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter id="iconTabFilter2" key="" icon="sap-icon://inspection">
                        <content>
							<f:SimpleForm id="iconTabFilter2form" minWidth="1024" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1">
								<f:content>
									<Label id="label4" text="">
									</Label>
									<Text id="text4" text="" maxLines="0">
									</Text>
									<Label id="label5" text="">
									</Label>
									<Text id="text5" text="" maxLines="0">
									</Text>
									<Label id="label6" text="">
									</Label>
									<Text id="text6" text="" maxLines="0">
									</Text>
									<core:ExtensionPoint name="extIconTabFilterForm2"/>
								</f:content>
							</f:SimpleForm>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter id="iconTabFilter3" key="" icon="sap-icon://add">
                        <content>
							<f:SimpleForm id="iconTabFilter3form" minWidth="1024" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1">
								<f:content>
								
									<core:ExtensionPoint name="extIconTabFilterForm3"/>
								</f:content>
							</f:SimpleForm>
                        </content>
                    </IconTabFilter>
    				<core:ExtensionPoint name="extIconTabFilter"/>
                </items>
            </IconTabBar>
    		<core:ExtensionPoint name="extDetail"/>
        </content>
        <footer id="detailFooter">
            <Toolbar id="detailToolbar">
                <content>
                    <ToolbarSpacer id="toolbarSpacer">
                    </ToolbarSpacer>
                    <Button id="actionButton" press="openActionSheet" icon="sap-icon://action">
                    </Button>
                </content>
            </Toolbar>
        </footer>
    </Page>
</mvc:View>